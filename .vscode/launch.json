{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Debug nodejsServerApp",
			"type": "node",
			"request": "launch",
			"args": ["${workspaceRoot}/nodejsServerApp/index.ts"],
			"runtimeArgs": ["-r", "ts-node/register"],
			"cwd": "${workspaceRoot}/nodejsServerApp",
			"protocol": "inspector",
			"internalConsoleOptions": "openOnSessionStart",
			"env": {
                "TS_NODE_IGNORE": "false",
                "TS_NODE_PROJECT":"tsconfig.json"
			}
		}
	]
}
